    \tr<\th<三.不定.青>\td<\mk;[ъа]\rd<[к]>[а̷]\rd<[т]>[ө̄]\rd<[п]>[о]>\td<\mk;[ъа][до̄]\rd<[к]>[а̷]\rd<[т]>[ө̄]\rd<[п]>[о]>\td<\mk;[ъа]\rd<[к]>[а̷]\rd<[т]>[ө̄]\rd<[п]>[а̷но]>\td<\mk;[ъа][до̄]\rd<[к]>[а̷]\rd<[т]>[ө̄]\rd<[п]>[а̷но]>>
    \tr<\th<二.赤>\td<\mk;[се]\rd<[к]>[а̷]\rd<[т]>[ө̄]\rd<[п]>>\td<\mk;[се][до̄]\rd<[к]>[а̷]\rd<[т]>[ө̄]\rd<[п]>>\td<\mk;[се]\rd<[к]>[а̷]\rd<[т]>[ө̄]\rd<[п]>[ан]>\td<\mk;[се][до̄]\rd<[к]>[а̷]\rd<[т]>[ө̄]\rd<[п]>[ан]>>
