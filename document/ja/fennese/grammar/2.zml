    \tr<\th<一単.青>\td<\mk;[йо]\rd<[к]>[а̷]\rd<[т]>[ө̄]\rd<[п]>[о]>\td<\mk;[йо][до̄]\rd<[к]>[а̷]\rd<[т]>[ө̄]\rd<[п]>[о]>\td<\mk;[йо]\rd<[к]>[а̷]\rd<[т]>[ө̄]\rd<[п]>[а̷но]>\td<\mk;[йо][до̄]\rd<[к]>[а̷]\rd<[т]>[ө̄]\rd<[п]>[а̷но]>>
